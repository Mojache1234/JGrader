<div class="page-header">
  <h1>Sections</h1>
  <a href="/teacher/section/create"></a>
</div>
<div class="table-responsive">
  <table class="table table-striped">
    <thead>
      <tr>
        <th>Name</th>
        <th>Students</th>
        <th>Last Assignment</th>
      </tr>
    </thead>
    <tbody>
      <% if(rows.length == 0) { %>
        <tr>
          <td colspan="3">You have no sections!</td>
        </tr>
      <% } else {
          for(i in rows) {%>
            <tr>
              <td><a href="/teacher/section/<%= rows[i].id %>"><%= rows[i].name %></a></td>
              <td><%= rows[i].count %></td>
              <td><a href="#">Picture Lab</a></td>
            </tr>
       <% }
         } %>
    </tbody>
  </table>
</div>